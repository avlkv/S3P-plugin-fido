{
"plugin": {
    "reference": "fido",
    "type": "SOURCE",
    "filenames": ["fido.py"],
    "localstorage": true
},
"task": {
    "log": -1,
    "trigger": {
      "type": "SCHEDULE",
      "interval": "7 days"
    }
},
"middleware": {
    "modules": [
        { "order": 1, "name": "TimezoneSafeControl", "critical": true, "params": {}},
        { "order": 2, "name": "CutJunkCharactersFromDocumentText", "critical": true, "params": {
            "fields": ["text", "abstract"]
        }},
        { "order": 3, "name": "FilterOnlyNewDocumentWithDB", "critical": true, "params": {}},
        { "order": 4, "name": "SaveDocumentToDB", "critical": true, "params": {}}
    ],
    "bus": {
        "entities": []
    }
},
"payload": {
    "file": "fido.py",
    "class": "FIDO",
    "entry": {
        "point": "content",
        "params": [
          {"key": "webdriver", "value": {"type": "module", "name": "WebDriver", "bus": true}},
          {"key": "max_count_documents", "value": {"type": "const", "value": 500}},
          {"key": "num_scrolls", "value": {"type": "const", "value": 25}},
          {"key": "source_type", "value": {"type": "const", "value": "FILE"}},
          {"key": "categories", "value": {"type": "const", "value": {"FIDO Case Studies": "https://fidoalliance.org/content/case-study/",
                   "FIDO White Papers": "https://fidoalliance.org/content/white-paper/"}}}
        ]
    },
    "additional_methods": null
}
}